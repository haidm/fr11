                <block type="core/template" name="logo" as="logo" template="luannt3/logo.phtml"/>
            </block>
            <block type="core/text_list" name="left" as="left" >
                <block type="core/template" name="c" as="c"  template="luannt3/c.phtml"/>
            </block>
            <block type="core/text_list" name="content" as="content">
