<?xml version="1.0"?>
<config>
    <modules>
        <Hungbd_Demoproduct>
            <version>0.1.0</version>
        </Hungbd_Demoproduct>
    </modules>
    <frontend>
        <routers>
            <Demoproduct>
                <use>standard</use>
                <args>
                    <module>Hungbd_Demoproduct</module>
                    <frontName>demoproduct</frontName>
                </args>
            </Demoproduct>
        </routers>
    </frontend>
    <global>
        <models>
            <Demoproduct>
                <class>Hungbd_Demoproduct_Model</class>
            </Demoproduct>
        </models>
        <event>
            <catalog_product_collection_load_after>
                <observers>
                    <changeListPrice>
                        <class>demoproduct/observer</class>
                        <method>changeListPrice</method>
                    </changeListPrice>
                </observers>
            </catalog_product_collection_load_after>
        </event>
    </global>
</config>